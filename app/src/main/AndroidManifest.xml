<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.contestssubscription">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.contestssubscription.data.UserApplication"
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ContestsSubscription">
        <activity
            android:name="com.example.contestssubscription.MainActivity"
            android:exported="true"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>